  "version": "0.2.0",
  "configurations": [
    {
      "name": "startup_app",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_prod.dart",
      "args": ["--flavor", "prod", "target", "lib/main_prod.dart"],
      "flutterMode": "debug"
    },
    {
      "name": "startup_app_dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "args": ["--flavor", "dev", "target", "lib/main_dev.dart"],
      "flutterMode": "debug"
    },

    {
      "name": "startup_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "startup_app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
